<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
  <switch>
    <case
      latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
      >
      <Key
        latin:keySpec="&#x1C5F;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C65;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C70;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C5D;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C5C;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C66;"
        latin:keyLabelFlags="fontNormal" />
    </case>

    <default>
      <Key
        latin:keySpec="&#x1C76;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C79;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C7D;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C6A;"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C7A;"
        latin:additionalMoreKeys=";"
        latin:keyLabelFlags="fontNormal"/>
      <Key
        latin:keySpec="&#x1C77;"
        latin:keyLabelFlags="fontNormal"/>
    </default>
  </switch>
</merge>
