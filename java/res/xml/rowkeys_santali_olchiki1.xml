<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
	<switch>
		<case
			latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
			>
			<Key
				latin:keySpec="&#x1C6F;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C7C;"
				latin:moreKeys="-"
				latin:keyLabelFlags="fontNormal" />
			<Key
				latin:keySpec="&#x1C60;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C5E;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C61;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C6B;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C5A;"
				latin:keyLabelFlags="fontNormal" />
			<Key
				latin:keySpec="&#x1C64;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C72;"
				latin:keyLabelFlags="fontNormal" /> 
			<Key
				latin:keySpec="&#x1C50;"
				latin:keyLabelFlags="fontNormal" />


		</case>

		<default> 
			<Key
				latin:keySpec="&#x1C51;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C52;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C53;"
				latin:keyLabelFlags="fontNormal"
				latin:additionalMoreKeys="#" />
			<Key
				latin:keySpec="&#x1C54;"
				latin:keyLabelFlags="fontNormal"
				latin:additionalMoreKeys="$" />
			<Key
				latin:keySpec="&#x1C55;"
				latin:keyLabelFlags="fontNormal"
				latin:additionalMoreKeys="%" />
			<Key
				latin:keySpec="&#x1C56;"
				latin:keyLabelFlags="fontNormal"
				latin:additionalMoreKeys="^" />
			<Key
				latin:keySpec="&#x1C57;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C58;"
				latin:keyLabelFlags="fontNormal"
				latin:additionalMoreKeys="*" />
			<Key
				latin:keySpec="&#x1C59;"
				latin:keyLabelFlags="fontNormal"
				latin:additionalMoreKeys="(" />
			<Key
				latin:keySpec="&#x1C50;"
				latin:keyLabelFlags="fontNormal"
				latin:additionalMoreKeys=")" />


		</default>
	</switch>
</merge>
