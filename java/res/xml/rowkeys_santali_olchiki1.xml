<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
            >
    <Key
        latin:keySpec="&#x1C51;"
        latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="!" />
    <Key
        latin:keySpec="&#x1C52;"
        latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="@" />
    <Key
        latin:keySpec="&#x1C53;"
        latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="#" />
    <Key
        latin:keySpec="&#x1C54;"
	latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="$" />
    <Key
        latin:keySpec="&#x1C55;"
	latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="%" />
    <Key
        latin:keySpec="&#x1C56;"
	latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="^" />
    <Key
        latin:keySpec="&#x1C57;"
	latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="&" />
    <Key
        latin:keySpec="&#x1C58;"
	latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="*" />
    <Key
        latin:keySpec="&#x1C59;"
	latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys="(" />
    <Key
        latin:keySpec="&#x1C50;"
	latin:keyLabelFlags="fontNormal"
        latin:additionalMoreKeys=")" />
    <Key
        latin:keySpec="&#x1C72;"
	latin:keyLabelFlags="fontNormal" />
    

    </case>
    
    <default>  
    <Key
        latin:keySpec="1"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="2"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="3"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="4"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="5"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="6"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="7"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="8"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="9"
	latin:keyLabelFlags="fontNormal"/>
    <Key
        latin:keySpec="0"
	latin:keyLabelFlags="fontNormal"/>

   </default>
   </switch>
</merge>
