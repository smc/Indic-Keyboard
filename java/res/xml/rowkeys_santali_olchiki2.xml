<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
	<switch>
		<case
			latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
			>
			<Key
				latin:keySpec="&#x1C67;"
				latin:keyLabelFlags="fontNormal"/>  
			<Key
				latin:keySpec="&#x1C63;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C6E;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C68;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C74;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C6D;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C69;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="["
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="]"
				latin:keyLabelFlags="fontNormal"/>



		</case>

		<default>
			<Key
				latin:keySpec="&#x1C75;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C71;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C62;"
				latin:keyLabelFlags="fontNormal"/>
			<Key 
				latin:keySpec="&#x1C5B;"
				latin:keyLabelFlags="fontNormal"/>
			<Key 
				latin:keySpec="&#x1C73;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C78;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="&#x1C6C;"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="{"
				latin:keyLabelFlags="fontNormal"/>
			<Key
				latin:keySpec="}"
				latin:keyLabelFlags="fontNormal"/>


		</default>
	</switch>

</merge>
