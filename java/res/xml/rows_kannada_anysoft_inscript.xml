<merge xmlns:latin="http://schemas.android.com/apk/res-auto" >

    <include latin:keyboardLayout="@xml/key_styles_common" />

    <Row latin:keyWidth="9.09%p" >
        <include latin:keyboardLayout="@xml/rowkeys_kannada_anysoft_inscript1" />
    </Row>
    <Row latin:keyWidth="9.09%p" >
        <include latin:keyboardLayout="@xml/rowkeys_kannada_anysoft_inscript2" />
    </Row>
    <Row latin:keyWidth="9.09%p" >
        <Key
            latin:keyStyle="shiftKeyStyle"
            latin:keyWidth="9.09%p"
            latin:visualInsetsRight="1%p" />

        <include latin:keyboardLayout="@xml/rowkeys_kannada_anysoft_inscript3" />

        <Key
            latin:keyStyle="deleteKeyStyle"
            latin:keyWidth="9.09%p"
            latin:visualInsetsLeft="1%p" />
    </Row>

    <include latin:keyboardLayout="@xml/row_qwerty4" />

</merge>