<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key
                latin:keySpec="&#x09A7;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x098A;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0988;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09DC;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09A0;"
                latin:keyHintLabel="&#x09F3;"
                latin:additionalMoreKeys="&#x09F3;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0990;"
                latin:keyHintLabel="&#x099E;"
                latin:additionalMoreKeys="&#x099E;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0989;"
                latin:keyHintLabel="&#x09CE;"
                latin:additionalMoreKeys="&#x09CE;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0987;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0994;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09AB;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09C8;"
                latin:keyLabelFlags="fontNormal" />
        </case>

        <default>
            <Key
                latin:keySpec="&#x09A6;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09C2;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09C0;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09B0;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x099F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x098F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09C1;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09BF;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0993;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09AA;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x09C7;"
                latin:keyLabelFlags="fontNormal" />
         </default>
    </switch>
</merge>
