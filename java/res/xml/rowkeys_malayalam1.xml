<merge
    xmlns:latin="http://schemas.android.com/apk/res/in.androidtweak.inputmethod.indic"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
        >
            <!--ൌ, ഔ -->
            <Key
                latin:keyLabel="&#x0D14;"
                latin:moreKeys="&#x0D4C;"
                latin:keyHintLabel="&#x0D4C;" />
            <!-- വ, വ്വ -->
            <Key
                latin:keyLabel="&#x0D35;"
                latin:moreKeys="&#x0D35;&#x0D4D;&#x0D35;" />
            <!-- േ, ഏ -->
            <Key
                latin:keyLabel="&#x0D47;"
                latin:moreKeys="&#x0D0F;"
                latin:keyHintLabel="&#x0D0F;" />
            <!-- റ, ഋ,ര്‍‍ -->
            <Key
                latin:keyLabel="&#x0D31;"
                latin:moreKeys="&#x0D0B;, &#x0D30;&#x0D4D;&#x200D;"
                latin:keyHintLabel="&#x0D0B;" />
            <!-- ഥ, ഠ -->
            <Key
                latin:keyLabel="&#x0D25;"
                latin:moreKeys="&#x0D20;"
                latin:keyHintLabel="&#x0D20;" />
            <!-- യ, യ്യ -->
            <Key
                latin:keyLabel="&#x0D2F;"
                latin:moreKeys="&#x0D2F;&#x0D4D;&#x0D2F;"  />
            <!-- ൂ, ഊ -->
            <Key
                latin:keyLabel="&#x0D42;"
                latin:moreKeys="&#x0D0A;"
                latin:keyHintLabel="&#x0D0A;" />
            <!-- ീ, ഈ -->
            <Key
                latin:keyLabel="&#x0D40;"
                latin:moreKeys="&#x0D08;"
                latin:keyHintLabel="&#x0D08;" />
            <!-- ോ, ഓ -->
            <Key
                latin:keyLabel="&#x0D4B;"
                latin:moreKeys="&#x0D13;"
                latin:keyHintLabel="&#x0D13;" />
            <!-- മ്പ -->
            <Key
                latin:keyLabel="&#x0D2E;&#x0D4D;&#x0D2A;" />
        </case>

        <default>
            <!--ൈ, ഐ -->
            <Key
                latin:keyLabel="&#x0D10;"
                latin:moreKeys="&#x0D48;"
                latin:keyHintLabel="&#x0D48;" />
            <!-- വ, വ്വ -->
            <Key
                latin:keyLabel="&#x0D35;"
                latin:moreKeys="&#x0D35;&#x0D4D;&#x0D35;" />
            <!-- െ, എ -->
            <Key
                latin:keyLabel="&#x0D46;"
                latin:moreKeys="&#x0D0E;"
                latin:keyHintLabel="&#x0D0E;" />
            <!--ര, ൃ -->
            <Key
                latin:keyLabel="&#x0D30;"
                latin:moreKeys="&#x0D43;"
                latin:keyHintLabel="&#x0D43;" />
            <!-- ത, ട, ട്ട, ത്ത, ണ്ട, ന്ത  -->
            <Key
                latin:keyLabel="&#x0D24;"
                latin:moreKeys="&#x0D1F;,&#x0D1F;&#x0D4D;&#x0D1F;,&#x0D24;&#x0D4D;&#x0D24;,&#x0D23;&#x0D4D;&#x0D1F;,&#x0D28;&#x0D4D;&#x0D24;"
                latin:keyHintLabel="&#x0D1F;" />
            <!-- യ, യ്യ -->
            <Key
                latin:keyLabel="&#x0D2F;"
                latin:moreKeys="&#x0D2F;&#x0D4D;&#x0D2F;"  />
            <!-- ു, ഉ -->
            <Key
                latin:keyLabel="&#x0D41;"
                latin:moreKeys="&#x0D09;"
                latin:keyHintLabel="&#x0D09;" />
            <!-- ി, ഇ -->
            <Key
                latin:keyLabel="&#x0D3F;"
                latin:moreKeys="&#x0D07;"
                latin:keyHintLabel="&#x0D07;"
                />
            <!-- ൊ, ഒ -->
            <Key
                latin:keyLabel="&#x0D4A;"
                latin:moreKeys="&#x0D12;"
                latin:keyHintLabel="&#x0D12;"
                />
            <!-- പ, പ്പ -->
            <Key
                latin:keyLabel="&#x0D2A;"
                latin:moreKeys="&#x0D2A;&#x0D4D;&#x0D2A;" />
         </default>
    </switch>
</merge>
